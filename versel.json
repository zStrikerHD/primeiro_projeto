{
    "version": 2,
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/auth",
        "dest": "/api/auth.js"
      },
      {
        "src": "/auth/callback",
        "dest": "/api/callback.js"
      }
    ]
  }
  